
<T3DataStructure>
    <meta>
        <langDisable>1</langDisable>
    </meta>
    <sheets>
        <sDEF>
            <ROOT>
                <TCEforms>
                    <sheetTitle>Basic</sheetTitle>
                </TCEforms>
                <type>array</type>
                <el>
                    <settings.width>
                        <TCEforms>
                            <label>Slider Width(px)</label>
                            <config>
                                <type>input</type>
                                <eval>trim</eval>
                            </config>
                        </TCEforms>
                    </settings.width>
                    <settings.height>
                        <TCEforms>
                            <label>Slider Height(px)</label>
                            <config>
                                <type>input</type>
                                <eval>trim</eval>
                            </config>
                        </TCEforms>
                    </settings.height>
                    <settings.isResponsive>
                        <TCEforms>
                            <label>Responsive</label>
                            <config>
                                <type>check</type>
                                <eval>trim</eval>
                            </config>
                        </TCEforms>
                    </settings.isResponsive>
                    <settings.isFullWidthSlider>
                        <TCEforms>
                            <label>Full-width slider</label>
                            <config>
                                <type>check</type>
                                <eval>trim</eval>
                            </config>
                        </TCEforms>
                    </settings.isFullWidthSlider>
                    <settings.responsive_under>
                        <TCEforms>
                            <label>Responsive under(px)</label>
                            <config>
                                <type>input</type>
                                <eval>trim</eval>
                            </config>
                        </TCEforms>
                    </settings.responsive_under>
                    <settings.layer_container>
                        <TCEforms>
                            <label>Layers Container(px)</label>
                            <config>
                                <type>input</type>
                                <eval>trim</eval>
                            </config>
                        </TCEforms>
                    </settings.layer_container>
                    <settings.teaser>
                        <TCEforms>
                            <label>Slider</label>
                            <config>
                                <type>select</type>
                                <itemsProcFunc>PITS\PitsLayerslider\Controller\LayersliderController->select</itemsProcFunc>
                                <size>5</size>
                                <maxitems>15</maxitems>
                            </config>
                        </TCEforms>
                    </settings.teaser>
                </el>
            </ROOT>
        </sDEF>
        <Slideshow>
            <ROOT>
                <TCEforms>
                    <sheetTitle>Slideshow</sheetTitle>
                </TCEforms>
                <type>array</type>
                <el>
                    <settings.isSlideshowAutoStart>
                        <TCEforms>
                            <label>Automatically start slideshow</label>
                            <config>
                                <type>check</type>
                                <eval>trim</eval>
                            </config>
                        </TCEforms>
                    </settings.isSlideshowAutoStart>
                    <settings.isPauseOnHover>
                        <TCEforms>
                            <label>Pause on hover</label>
                            <config>
                                <type>check</type>
                                <eval>trim</eval>
                            </config>
                        </TCEforms>
                    </settings.isPauseOnHover>
                    <settings.isAnimateFirstSlide>
                        <TCEforms>
                            <label>Animate first slide</label>
                            <config>
                                <type>check</type>
                                <eval>trim</eval>
                            </config>
                        </TCEforms>
                    </settings.isAnimateFirstSlide>
                    <settings.isRandomSlide>
                        <TCEforms>
                            <label>Random slideshow</label>
                            <config>
                                <type>check</type>
                                <eval>trim</eval>
                            </config>
                        </TCEforms>
                    </settings.isRandomSlide>
                    <settings.isTwoWaySlideShow>
                        <TCEforms>
                            <label>Two way slideshow</label>
                            <config>
                                <type>check</type>
                                <eval>trim</eval>
                            </config>
                        </TCEforms>
                    </settings.isTwoWaySlideShow>
                    <settings.loops>
                        <TCEforms>
                            <label>Loops</label>
                            <config>
                                <type>select</type>
                                <items type="array">
                                    <numIndex index="0">
                                        <numIndex index="0">0</numIndex>
                                        <numIndex index="1">0</numIndex>
                                    </numIndex>
                                    <numIndex index="1">
                                        <numIndex index="0">1</numIndex>
                                        <numIndex index="1">1</numIndex>
                                    </numIndex>
                                    <numIndex index="2">
                                        <numIndex index="0">2</numIndex>
                                        <numIndex index="1">2</numIndex>
                                    </numIndex>
                                    <numIndex index="3">
                                        <numIndex index="0">3</numIndex>
                                        <numIndex index="1">3</numIndex>
                                    </numIndex>
                                    <numIndex index="4">
                                        <numIndex index="0">4</numIndex>
                                        <numIndex index="1">4</numIndex>
                                    </numIndex>
                                    <numIndex index="5">
                                        <numIndex index="0">5</numIndex>
                                        <numIndex index="1">5</numIndex>
                                    </numIndex>
                                    <numIndex index="6">
                                        <numIndex index="0">6</numIndex>
                                        <numIndex index="1">6</numIndex>
                                    </numIndex>
                                    <numIndex index="7">
                                        <numIndex index="0">7</numIndex>
                                        <numIndex index="1">7</numIndex>
                                    </numIndex>
                                    <numIndex index="8">
                                        <numIndex index="0">8</numIndex>
                                        <numIndex index="1">8</numIndex>
                                    </numIndex>
                                    <numIndex index="9">
                                        <numIndex index="0">9</numIndex>
                                        <numIndex index="1">9</numIndex>
                                    </numIndex>
                                    <numIndex index="10">
                                        <numIndex index="0">10</numIndex>
                                        <numIndex index="1">10</numIndex>
                                    </numIndex>
                                </items>
                                <minitems>0</minitems>
                                <maxitems>1</maxitems>
                                <size>1</size>
                            </config>
                        </TCEforms>
                    </settings.loops>
                    <settings.isForceLoop>
                        <TCEforms>
                            <label>Force the number of loops</label>
                            <config>
                                <type>check</type>
                                <eval>trim</eval>
                            </config>
                        </TCEforms>
                    </settings.isForceLoop>
                    <settings.isAutoPlayVideo>
                        <TCEforms>
                            <label>Automatically play videos</label>
                            <config>
                                <type>check</type>
                                <eval>trim</eval>
                            </config>
                        </TCEforms>
                    </settings.isAutoPlayVideo>
                    <settings.autoPlaySlideshowSelector>
                        <TCEforms>
                            <label>Automatically pause slideshow</label>
                            <config>
                               <type>select</type>
                                <items type="array">
                                    <numIndex index="0">
                                        <numIndex index="0">auto</numIndex>
                                        <numIndex index="1">auto</numIndex>
                                    </numIndex>
                                    <numIndex index="1">
                                        <numIndex index="0">enabled</numIndex>
                                        <numIndex index="1">enabled</numIndex>
                                    </numIndex>
                                    <numIndex index="2">
                                        <numIndex index="0">disabled</numIndex>
                                        <numIndex index="1">disabled</numIndex>
                                    </numIndex>
                                </items>
                                <minitems>0</minitems>
                                <maxitems>1</maxitems>
                                <size>1</size>
                            </config>
                        </TCEforms>
                    </settings.autoPlaySlideshowSelector>
                </el>
            </ROOT>
        </Slideshow>
         <Appearance>
            <ROOT>
                <TCEforms>
                    <sheetTitle>Appearance</sheetTitle>
                </TCEforms>
                <type>array</type>
                <el>
                    <settings.select_skin>
                        <TCEforms>
                            <label>Skin</label>
                            <config>
                               <type>select</type>
                                <items type="array">
                                    <numIndex index="0">
                                        <numIndex index="0">borderlessdark</numIndex>
                                        <numIndex index="1">borderlessdark</numIndex>
                                    </numIndex>
                                    <numIndex index="1">
                                        <numIndex index="0">borderlessdark3d</numIndex>
                                        <numIndex index="1">borderlessdark3d</numIndex>
                                    </numIndex>
                                    <numIndex index="2">
                                        <numIndex index="0">borderlesslight</numIndex>
                                        <numIndex index="1">borderlesslight</numIndex>
                                    </numIndex>
                                    <numIndex index="3">
                                        <numIndex index="0">carousel</numIndex>
                                        <numIndex index="1">carousel</numIndex>
                                    </numIndex>
                                    <numIndex index="4">
                                        <numIndex index="0">darkskin</numIndex>
                                        <numIndex index="1">darkskin</numIndex>
                                    </numIndex>
                                    <numIndex index="5">
                                        <numIndex index="0">defaultskin</numIndex>
                                        <numIndex index="1">defaultskin</numIndex>
                                    </numIndex>
                                    <numIndex index="6">
                                        <numIndex index="0">fullwidth</numIndex>
                                        <numIndex index="1">fullwidth</numIndex>
                                    </numIndex>
                                    <numIndex index="7">
                                        <numIndex index="0">fullwidthdark</numIndex>
                                        <numIndex index="1">fullwidthdark</numIndex>
                                    </numIndex>
                                    <numIndex index="8">
                                        <numIndex index="0">glass</numIndex>
                                        <numIndex index="1">glass</numIndex>
                                    </numIndex>
                                    <numIndex index="9">
                                        <numIndex index="0">lightskin</numIndex>
                                        <numIndex index="1">lightskin</numIndex>
                                    </numIndex>
                                    <numIndex index="10">
                                        <numIndex index="0">minimal</numIndex>
                                        <numIndex index="1">minimal</numIndex>
                                    </numIndex>
                                    <numIndex index="11">
                                        <numIndex index="0">noskin</numIndex>
                                        <numIndex index="1">noskin</numIndex>
                                    </numIndex>
                                    <numIndex index="12">
                                        <numIndex index="0">preview</numIndex>
                                        <numIndex index="1">preview</numIndex>
                                    </numIndex>
                                </items>
                                <minitems>0</minitems>
                                <maxitems>1</maxitems>
                                <size>1</size>
                            </config>
                        </TCEforms>
                    </settings.select_skin>
                     <settings.bgColor>
                        <TCEforms>
                            <label>Background color</label>
                            <config>
                                <type>input</type>
                                <eval>trim</eval>
                            </config>
                        </TCEforms>
                    </settings.bgColor>
                    <settings.sliderStyle>
                        <TCEforms>
                            <label>Slider style</label>
                            <config>
                                <type>input</type>
                                <eval>trim</eval>
                            </config>
                        </TCEforms>
                    </settings.sliderStyle>
                </el>
            </ROOT>
        </Appearance>
         <Navigation>
            <ROOT>
                <TCEforms>
                    <sheetTitle>Navigation</sheetTitle>
                </TCEforms>
                <type>array</type>
                <el>
                    <settings.isPrevNextEnable>
                        <TCEforms>
                            <label>Prev and Next buttons</label>
                            <config>
                                <type>check</type>
                                <eval>trim</eval>
                            </config>
                        </TCEforms>
                    </settings.isPrevNextEnable>
                    <settings.isStartStopEnable>
                        <TCEforms>
                            <label>Start and Stop buttons</label>
                            <config>
                                <type>check</type>
                                <eval>trim</eval>
                            </config>
                        </TCEforms>
                    </settings.isStartStopEnable>
                     <settings.isNavButtonsEnable>
                        <TCEforms>
                            <label>Navigation buttons </label>
                            <config>
                                <type>check</type>
                                <eval>trim</eval>
                            </config>
                        </TCEforms>
                    </settings.isNavButtonsEnable>
                    <settings.isPrevNextOnHover>
                        <TCEforms>
                            <label>Prev and next buttons on hover</label>
                            <config>
                                <type>check</type>
                                <eval>trim</eval>
                            </config>
                        </TCEforms>
                    </settings.isPrevNextOnHover>
                    <settings.isBottomNavOnHover>
                        <TCEforms>
                            <label>Bottom navigation on hover</label>
                            <config>
                                <type>check</type>
                                <eval>trim</eval>
                            </config>
                        </TCEforms>
                    </settings.isBottomNavOnHover>
                    <settings.isBarTimerEnable>
                        <TCEforms>
                            <label>Show bar timer</label>
                            <config>
                                <type>check</type>
                                <eval>trim</eval>
                            </config>
                        </TCEforms>
                  </settings.isBarTimerEnable>
                    <settings.isCircleTimerEnable>
                        <TCEforms>
                            <label>Show circle timer</label>
                            <config>
                                <type>check</type>
                                <eval>trim</eval>
                            </config>
                        </TCEforms>
                    </settings.isCircleTimerEnable>
                    <settings.thumbil_navigation>
                        <TCEforms>
                            <label>Thumbnail navigation </label>
                            <config>
                                <type>select</type>
                                <items type="array">
                                    <numIndex index="0">
                                        <numIndex index="0">disabled</numIndex>
                                        <numIndex index="1">disabled</numIndex>
                                    </numIndex>
                                    <numIndex index="1">
                                        <numIndex index="0">hover</numIndex>
                                        <numIndex index="1">hover</numIndex>
                                    </numIndex>
                                    <numIndex index="2">
                                        <numIndex index="0">always</numIndex>
                                        <numIndex index="1">always</numIndex>
                                    </numIndex>
                                </items>
                                <minitems>0</minitems>
                                <maxitems>1</maxitems>
                                <size>1</size>
                            </config>
                        </TCEforms>
                    </settings.thumbil_navigation>
                    <settings.thumbnail_width>
                        <TCEforms>
                            <label>Thumbnail width</label>
                            <config>
                                <type>input</type>
                                <eval>trim</eval>
                            </config>
                        </TCEforms>
                    </settings.thumbnail_width>
                    <settings.thumbnail_height>
                        <TCEforms>
                            <label>Thumbnail height</label>
                            <config>
                                <type>input</type>
                                <eval>trim</eval>
                            </config>
                        </TCEforms>
                    </settings.thumbnail_height>
                    <settings.thumbnail_container_width>
                        <TCEforms>
                            <label>Thumbnail container width</label>
                            <config>
                                <type>input</type>
                                <eval>trim</eval>
                            </config>
                        </TCEforms>
                    </settings.thumbnail_container_width>
                    <settings.thumbnail_opacity>
                        <TCEforms>
                            <label>Thumbnail active opacity</label>
                            <config>
                                <type>input</type>
                                <eval>trim</eval>
                            </config>
                        </TCEforms>
                    </settings.thumbnail_opacity>
                    <settings.thumbnail_inactive_opacity>
                        <TCEforms>
                            <label>Thumbnail inactive opacity</label>
                            <config>
                                <type>input</type>
                                <eval>trim</eval>
                            </config>
                        </TCEforms>
                    </settings.thumbnail_inactive_opacity>
                </el>
            </ROOT>
        </Navigation>
                 <Misc>
            <ROOT>
                <TCEforms>
                    <sheetTitle>Misc</sheetTitle>
                </TCEforms>
                <type>array</type>
                <el>
                   <settings.isImagePreloadEnable>
                        <TCEforms>
                            <label>Image preload </label>
                            <config>
                                <type>check</type>
                                <eval>trim</eval>
                            </config>
                        </TCEforms>
                    </settings.isImagePreloadEnable>
                    <settings.isRealUrlEnable>
                        <TCEforms>
                            <label>Thumbnail inactive opacity</label>
                            <config>
                                <type>check</type>
                                <eval>trim</eval>
                            </config>
                        </TCEforms>
                    </settings.isRealUrlEnable>
                </el>
            </ROOT>
        </Misc>
    </sheets>
</T3DataStructure>